            "id": 1, 
            "pid": 0,  父id
            "tagCateId": 1,   标签分类ID
            "tagTitle": "个性标签1",   标签名称
            "tagStatus": 1,   标签状态
            "tagIcon": null,   标签图标
            "tagSort": 0,
            "createdAt": null,
            "updatedAt": null,
            "deletedAt": null,
            "tagsList": [      二级标签
                {
                    "id": 5,
                    "pid": 1,
                    "tagCateId": null,
                    "tagTitle": "个性标签二级1",
                    "tagStatus": 1,
                    "tagIcon": null,
                    "tagSort": 0,
                    "createdAt": null,
                    "updatedAt": null,
                    "deletedAt": null,
                    "tagsList": []
                }
            ]
        },
        {
            "id": 2,
